//count the number of even numbers from 1 to 10

import java.lang.System;
import java.util.Scanner;

class welcome
{
    public static void main(String args[])
    {
        int count = 0;
        for(int i = 1; i<=10; i++)
        {
            if(i%2==0)
            {
                count++; 
            }
        }
        System.out.println("The total number of even numbers between 1 and 10 is "+ count);
    }
}
