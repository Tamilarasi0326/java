// print even number between 1 to 10 using for loop and if else.

import java.lang.System;
import java.util.Scanner;

class welcome
{
    public static void main(String args[])
    {
        Scanner s = new Scanner(System.in);
        for(int i = 1; i<=10; i++)
        {
            if(i%2==0)
            {
                System.out.println(i);
            }
        }
    }
}
